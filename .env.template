OPENAI_API_KEY=your_openai_api_key_here

# Optional: Enable debug mode for plugin discovery
# PROMPTMATRYOSHKA_DEBUG=1

# Optional: Set custom model for LLM plugins
# OPENAI_MODEL=gpt-4